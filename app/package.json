{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:studio": "concurrently \"pnpm dev\" \"pnpm db:studio\"",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx src/lib/server/db/seed.ts",
		"eval": "tsx --env-file=.env scripts/run-eval.ts",
		"export": "tsx --env-file=.env scripts/export-eval.ts",
		"scores-summary": "tsx --env-file=.env scripts/generate-scores-summary.ts",
		"validate-scores": "tsx --env-file=.env scripts/validate-scores.ts",
		"publish": "pnpm export && pnpm scores-summary && pnpm validate-scores"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"@types/node": "^22",
		"concurrently": "^9.2.1",
		"dotenv": "^17.2.3",
		"drizzle-kit": "^0.31.5",
		"drizzle-orm": "^0.44.6",
		"mdsvex": "^0.12.6",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.14",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vite": "^7.1.10"
	},
	"dependencies": {
		"@types/d3": "^7.4.3",
		"chart.js": "^4.4.8",
		"d3": "^7.9.0",
		"magisterium": "^0.0.5",
		"postgres": "^3.4.7",
		"yaml": "^2.8.1",
		"zod": "^4.1.12"
	}
}
